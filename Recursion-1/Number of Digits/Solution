/* Code that is supposed to be fixed:
public class Solution {

	public static int count(int n){
		if(n == 0){
			return 1;
		}
		int smallAns = count(n / 10);
		return smallAns+1;
	}

}
*/

public class Solution {

	public static int count(int n){
		if(n == 0){
			return 0;
		}
		int smallAns = count(n / 10);
		return smallAns+1;
	}

}
